    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.4",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "prettier": "3.0.2",
    "ts-jest": "^29.1.1"
  }
